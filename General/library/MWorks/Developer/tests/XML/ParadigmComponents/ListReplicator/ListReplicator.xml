<?xml version="1.0"?>
<monkeyml version="1.0">
    <io_devices tag="IO Devices"></io_devices>
    <variables tag="Variables">
        <variable tag="list_var" scope="local" logging="when_changed" default_value="DEFAULT" type="string"></variable>
    </variables>
    <sounds tag="Sounds"></sounds>
    <stimuli tag="Stimuli"></stimuli>
    <filters tag="Filters"></filters>
    <optimizers tag="Optimizers"></optimizers>
    <experiment tag="New Experiment" full_name="" description="">
        <protocol tag="New Protocol" nsamples="1" sampling_method="cycles" selection="sequential">
            <list_replicator tag="New List Replicator" values="1,-2.2,abc,&quot;Hello, world!&quot;, 1.234 ,-12  " variable="list_var">
                <trial tag="New List" nsamples="1" sampling_method="cycles" selection="sequential">
                    <action type="report" tag="Report" message="list_var = $list_var"></action>
                </trial>
            </list_replicator>
        </protocol>
    </experiment>
</monkeyml>