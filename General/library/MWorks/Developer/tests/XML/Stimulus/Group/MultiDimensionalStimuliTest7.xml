                        <action type="dequeue_stimulus" stimulus="mainGroup[0][2][1]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[0][2][2]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[0][2][2] -- Image 5"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[0][2][2]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[1][0][0]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[1][0][0] -- Image 6"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[1][0][0]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[1][0][1]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[1][0][1] -- Image 7"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[1][0][1]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[1][0][2]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[1][0][2] -- Image 8"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[1][0][2]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[1][0][3]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[1][0][3] -- Image 9"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[1][0][3]"/>
                        <action type="update_stimulus_display"/>
                        <transition type="yield"/>
                    </task_system_state>
                </task_system>
            </block>
        </protocol>
    </experiment>
</monkeyml>
