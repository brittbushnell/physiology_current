    <experiment tag="MultiDimensionalStimuliTest">
        <protocol tag="Protocol" interruptible="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles">
            <block tag="Block" interruptible="YES" selection="sequential_ascending" nsamples="1" sampling_method="samples">
                <task_system tag="Task System_Count" interruptible="YES">
                    <task_system_state tag="stimON0_count" interruptible="YES">
                        <action type="queue_stimulus" stimulus="mainGroup[0][0][0]"/>
                        <action type="update_stimulus_display"/>
                        <action type="report" message="mainGroup[0][0][0] -- Image 0"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[0][0][0]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[0][1][0]"/>
                        <action type="update_stimulus_display"/>
                        <action type="report" message="mainGroup[0][1][0] -- Image 1"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[0][1][0]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[0][1][1]"/>
                        <action type="update_stimulus_display"/>
                        <action type="report" message="mainGroup[0][1][1] -- Image 2"/>
                        <action type="wait" duration="1000000"/>
                        <action type="dequeue_stimulus" stimulus="mainGroup[0][1][1]"/>
                        <action type="queue_stimulus" stimulus="mainGroup[0][2][0]"/>
                        <action type="update_stimulus_display"/>
			<action type="report" message="mainGroup[0][2][0] -- Image 3"/>
