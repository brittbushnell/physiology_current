                    <stimulus tag="XIM004" type="image_file" path="./4.png" x_size="25" y_size="25" x_position="0" y_position="0" rotation="4*90/11"/>
                    <stimulus tag="XIM005" type="image_file" path="./5.png" x_size="25" y_size="25" x_position="0" y_position="0" rotation="5*90/11"/>
                </stimulus_group>
